{
  "name": "netmusic",
  "version": "0.1.0",
  "private": true,
  "main": "./src/main.js",
  "scripts": {
    "dev": "run-p serve start",
    "serve": "node ./server/index.js",
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build --dest ./server/public",
    "build:dev": "vue-cli-service build --mode develepment",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "init": "npm i -g rimraf npm-run-all && npm i && cd server && npm i && cd ..",
    "clear": "rimraf node_modules",
    "prebuild": "npm run test:unit",
    "reinstall": "npm run clear && npm i"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "eslint": "7.8.1",
    "eslint-plugin-html": "6.0.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "lint-staged": "10.2.11",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.js": [
      "eslint --fix"
    ]
  }
}
